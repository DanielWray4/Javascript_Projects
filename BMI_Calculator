// BMI Calculator using Javascript
// BMI = mass / (height * height) - Calculation used to create the BMI Totals

//Variables required to calculate BMI
let massMark = 78;
let heightMark = 1.69;
let massJohn = 92;
let heightJohn = 1.95;

// Creating two variables to add the totals 
let BMIMark = massMark / (heightMark * heightMark);

let BMIJohn = massJohn / (heightJohn * heightJohn);

//Printing the variables to the console
console.log(BMIMark);
console.log(BMIJohn);
